{
  "name": "ecommerce-mern",
  "version": "1.0.0",
  "description": "MERN eCommerce App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node backend/server.js",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:export": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tinsae/eCommerce-MERN.git"
  },
  "author": "TGA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tinsae/eCommerce-MERN/issues"
  },
  "homepage": "https://github.com/Tinsae/eCommerce-MERN#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": "15.11.x",
    "npm": "7.6.x"
  }
}
